#-------------------------------------------------------------------------------
# Subsection: Utility Flasks
#-------------------------------------------------------------------------------

Show
	ItemLevel < 67
	Rarity <= Magic
	BaseType = "Silver Flask" "Quicksilver Flask"
	SetTextColor 255 255 255 255 # Normal Highlight Text
	SetBackgroundColor 30 30 30 255 # Normal Highlight Background
	SetFontSize 45 # Yuge
	#BlockStrictness 3 Disable

Show
	ItemLevel < 67
	Rarity <= Magic
	Class = "Utility Flasks"
	SetBackgroundColor 0 0 0 255 # Default Background
	SetFontSize 41 # Big
	#BlockStrictness 3 Disable

# Show all until we have tiering
Hide
	Rarity <= Magic
	Class = "Utility Flask"
	SetBackgroundColor 0 0 0 255 # Default Background
	SetFontSize 41 # Big
	#BlockStrictness 2 Hide

